{
    "name": "pedrazadixon/laravel-simple-permissions",
    "description": "A simple package to manage permissions in Laravel",
    "keywords": [
        "laravel",
        "permissions",
        "roles"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pedrazadixon\\LaravelSimplePermissions\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dixon Pedraza",
            "email": "pedrazadixon@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "laravel/framework": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pedrazadixon\\LaravelSimplePermissions\\Providers\\LaravelSimplePermissionsProvider"
            ]
        }
    }
}